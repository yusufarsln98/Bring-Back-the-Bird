{
	"manifest_version": 2,
	"name": "Fuck X",
	"version": "1.1",
	"description": "Fixes Twitter's iconic Bird logo that spoiled Elon's constantly messed up.",
	"icons": {
		"16": "icon-16.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	},
	"content_scripts": [
		{
			"matches": ["https://twitter.com/*"],
			"js": ["content.js"]
		}
	],
	"permissions": ["scripting", "tabs"],
	"browser_action": {
		"default_popup": "popup.html"
	}
}
